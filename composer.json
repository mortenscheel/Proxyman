{
    "name": "mortenscheel/nginx-proxyman",
    "description": "CLI tool for nginx-proxy-manager",
    "license": "MIT",
    "keywords": [
        "nginx-proxy",
        "cli"
    ],
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^10.45",
        "laravel-zero/framework": "^10.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/termwind": "^1.3",
        "pestphp/pest": "^1.21.1",
        "phpro/grumphp": "^2.5",
        "yieldstudio/grumphp-laravel-pint": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "proxyman"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpro/grumphp": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
