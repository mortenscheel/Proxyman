{
    "name": "mortenscheel/proxyman",
    "description": "CLI tool for nginx-proxy-manager",
    "keywords": ["nginx-proxy", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/proxyman"]
}
